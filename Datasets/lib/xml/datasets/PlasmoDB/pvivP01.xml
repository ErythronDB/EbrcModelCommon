<datasets>

  <constant name="organismAbbrev" value="pvivP01"/>
  <constant name="referenceStrainOrganismAbbrev" value="pvivSal1"/>
  <constant name="familyRepOrganismAbbrev" value="pfal3D7"/>
  <constant name="strainAbbrev" value="P01"/>
  <constant name="projectName" value="PlasmoDB"/>
  <constant name="ncbiTaxonId" value="9000000036"/>
  <constant name="speciesNcbiTaxonId" value="5855"/>
  <constant name="genomeVersion" value="2015-06-18"/>
  <constant name="functAnnotVersion" value="2017-03-06"/>

  <dataset class="validateOrganismInfo">
    <prop name="organismAbbrev">$$organismAbbrev$$</prop>
    <prop name="strainAbbrev">$$strainAbbrev$$</prop>
