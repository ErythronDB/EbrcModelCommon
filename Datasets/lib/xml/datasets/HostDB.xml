<datasets>
  <constant name="projectName" value="HostDB"/>
  <constant name="genbankIsolateMapperVersion" value="2015-11"/> 

  <dataset class="organism">
    <prop name="organismAbbrev">hsapREF</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="ncbiTaxonId">9000000055</prop>
    <prop name="publicOrganismAbbrev">hsapREF</prop>
    <prop name="organismFullName">Homo sapiens REF</prop>
    <prop name="organismNameForFiles">HsapiensREF</prop>
    <prop name="speciesNcbiTaxonId">9606</prop>
    <prop name="isReferenceStrain">true</prop>
    <prop name="referenceStrainOrganismAbbrev">hsapREF</prop>
    <prop name="isAnnotatedGenome">true</prop>
    <prop name="hasTemporaryNcbiTaxonId">true</prop>
    <prop name="orthomclAbbrev">hsap</prop>
    <prop name="strainAbbrev">REF</prop>
    <prop name="genomeSource">gencode</prop>
    <prop name="taxonHierarchyForBlastxFilter">Eukaryota Hominidae Homo</prop>
    <prop name="annotationIncludesTRNAs">true</prop>
    <prop name="runExportPred">false</prop>
    <prop name="maxIntronSize">750000</prop>
    <prop name="isHugeGenome">true</prop>
    <prop name="genomeVersion">GRCh38</prop>
    <prop name="isFamilyRepresentative">true</prop>
    <prop name="familyRepOrganismAbbrev">hsapREF</prop>
    <prop name="familyNcbiTaxonIds">9604</prop>
    <prop name="familyNameForFiles">Hominidae</prop>
  </dataset>

  <dataset class="referenceStrain">
    <prop name="organismAbbrev">hsapREF</prop>
    <prop name="isAnnotatedGenome">true</prop>
  </dataset>

  <dataset class="organism">
    <prop name="organismAbbrev">mmusC57BL6J</prop>
    <prop name="projectName">$$projectName$$</prop>
    <prop name="ncbiTaxonId">9000000052</prop>
    <prop name="publicOrganismAbbrev">mmusC57BL6J</prop>
    <prop name="organismFullName">Mus musculus C57BL6J</prop>
    <prop name="organismNameForFiles">MmusculusC57BL6J</prop>
    <prop name="speciesNcbiTaxonId">10090</prop>
    <prop name="isReferenceStrain">true</prop>
    <prop name="referenceStrainOrganismAbbrev">mmusC57BL6J</prop>
    <prop name="isAnnotatedGenome">true</prop>
    <prop name="hasTemporaryNcbiTaxonId">true</prop>
    <prop name="orthomclAbbrev">mmus</prop>
    <prop name="strainAbbrev">C57BL6J</prop>
    <prop name="genomeSource">gencode</prop>
    <prop name="taxonHierarchyForBlastxFilter">Eukaryota Muridae Mus</prop>
    <prop name="annotationIncludesTRNAs">true</prop>
    <prop name="runExportPred">false</prop>
    <prop name="maxIntronSize">750000</prop>
    <prop name="isHugeGenome">true</prop>
    <prop name="genomeVersion">GRCm38</prop>
    <prop name="isFamilyRepresentative">true</prop>
    <prop name="familyRepOrganismAbbrev">mmusC57BL6J</prop>
    <prop name="familyNcbiTaxonIds">10066</prop>
    <prop name="familyNameForFiles">Muridae</prop>
  </dataset>

  <dataset class="referenceStrain">
    <prop name="organismAbbrev">mmusC57BL6J</prop>
    <prop name="isAnnotatedGenome">true</prop>
  </dataset>

<dataset class="orthomcl">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.13</prop>
  </dataset>

  <dataset class="orthomclPhyletic">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.13</prop>
  </dataset>

  <dataset class="orthomclTree">
    <prop name="projectName">$$projectName$$</prop>
    <prop name="version">5.13</prop>
  </dataset>

  <!-- load of all KEGG pathways -->
  <dataset class="Pathways">
    <prop name="version">2013.07</prop>
    <prop name="format">KEGG</prop>
    <prop name="source">KEGG</prop>
    <prop name="projectName">$$projectName$$</prop>
  </dataset>

  <dataset class="Pathways">
    <prop name="version">2017.02.07</prop>
    <prop name="format">BioCyc</prop>
    <prop name="source">MetaCyc</prop>
    <prop name="projectName">$$projectName$$</prop>
  </dataset> 

  <!-- load of all KEGG substances and compounds -->
<!--
  <dataset class="PathwayCompounds">
    <prop name="name">pubchem</prop>
    <prop name="version">2012.11</prop>
  </dataset>
-->
  <dataset class="EcNumberGenus">
    <prop name="version">2015-03-11</prop>
    <prop name="projectName">$$projectName$$</prop>
  </dataset>

  <dataset class="ISA"/>
  <dataset class="ISASimple"/>

  <dataset class="dumpMissingGenbankIsolateMappers">
    <prop name="version">$$genbankIsolateMapperVersion$$</prop>
  </dataset>

  <dataset class="ontologySynonyms">
    <prop name="version">$$genbankIsolateMapperVersion$$</prop>
    <prop name="name">genbankIsolates</prop>
  </dataset>

</datasets>
